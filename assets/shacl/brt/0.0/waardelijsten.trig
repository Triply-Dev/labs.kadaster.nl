brt: {

  brt:Breedteklasse
    owl:oneOf
      ( breedteklasse:05_3meter
        breedteklasse:3_6meter
        breedteklasse:6_12meter
        breedteklasse:12_50meter
        breedteklasse:50_125meter
        breedteklasse:groterdan125meter );
    rdfs:seeAlso "https://kadaster.github.io/imbrt/#domein-wa_breedteklasse"^^xsd:anyURI;
    rdfs:subClassOf skos:Concept;
    skos:definition "De breedte van het water ingedeeld in klassen."@nl;
    skos:prefLabel "Breedteklasse"@nl.

  breedteklasse:05_3meter
    skos:definition "De breedte van het water is groter dan of gelijk aan 0,5 meter en minder dan 3 meter."@nl;
    skos:prefLabel "0,5 - 3 meter"@nl.

  breedteklasse:3_6meter
    skos:definition "De breedte van het water is groter dan of gelijk aan 3 meter en minder dan 6 meter."@nl;
    skos:prefLabel "3 - 6 meter"@nl.

  breedteklasse:6_12meter
    skos:definition "De breedte van het water is groter dan of gelijk aan 6 meter en minder dan 12 meter."@nl;
    skos:prefLabel "6 - 12 meter"@nl.

  breedteklasse:12_50meter
    skos:definition "De breedte van het water is groter dan of gelijk aan 12 meter en minder dan 50 meter."@nl;
    skos:prefLabel "12 - 50 meter"@nl.

  breedteklasse:50_125meter
    skos:definition "De breedte van het water is groter dan of gelijk aan 50 meter en minder dan 125 meter."@nl;
    skos:prefLabel "50 - 125 meter"@nl.

  breedteklasse:groterdan125meter
    skos:definition "De breedte van het water is groter dan of gelijk aan 125 meter."@nl;
    skos:prefLabel "> 125 meter"@nl.



  brt:Brontype
    owl:oneOf
      ( brontype:externedata
        brontype:luchtfoto
        brontype:overig
        brontype:rd
        brontype:top10vector );
    rdfs:seeAlso "https://kadaster.github.io/imbrt/#domein-brontype"^^xsd:anyURI;
    rdfs:subClassOf skos:Concept;
    skos:definition "Het type van de gebruikte bron."@nl;
    skos:prefLabel "Brontype"@nl.

  brontype:externeData
    skos:definition "De data worden aangeleverd door externe leveranciers."@nl;
    skos:prefLabel "externe data"@nl.

  brontype:luchtfoto
    skos:definition "Een fotografische opname van een deel van het aardoppervlak, gemaakt vanuit een vliegtuig."@nl;
    skos:prefLabel "luchtfoto"@nl.

  brontype:overig
    skos:definition "De waarde van het objectkenmerk is bekend, maar anders dan de genoemde waarden."@nl;
    skos:prefLabel "overig"@nl.

  brontype:rd
    skos:definition "Net van vaste punten in Nederland. Deze vaste punten zijn de referentiepunten waarnaar overige metingen en karteringen kunnen verwijzen. De referentiepunten maken deel uit van een netwerk dat is vastgelegd in het systeem van de Rijksdriehoeksmeting (RD). Dit netwerk wordt beheerd en onderhouden door het Kadaster. ( www.kadaster.nl )."@nl;
    skos:prefLabel "RD"@nl.

  brontype:top10vector
    skos:definition "Digitaal bestand, bestaande uit gecodeerde vectoren, die tezamen de topografie van Nederland weergeven op de schaal 1:10.000. Voorloper van TOP10NL."@nl;
    skos:prefLabel "TOP10vector"@nl.



  brt:Functie
    owl:unionOf
      ( brt:FunctieReliëf
        brt:FunctieWaterdeel );
    rdfs:subClassOf skos:Concept;
    skos:prefLabel "Functie"@nl.


  brt:FunctieReliëf
    owl:oneOf
      ( functieReliëf:geluidWeren );
    rdfs:seeAlso "https://kadaster.github.io/imbrt/#domein-re_functie"^^xsd:anyURI;
    skos:definition "Aanduiding of het object een geluidswerende functie heeft."@nl;
    skos:prefLabel "Reliëf functie"@nl.

  functieReliëf:geluidWeren
    skos:definition "Het reliëf heeft de functie van geluidswering in de vorm van een langwerpige, opgeworpen hoogte."@nl;
    skos:prefLabel "geluid weren"@nl;
        skos:scopeNote """
<section lang="nl">
  <h1>Inwinningscriteria</h1>
  <p>Minimum lengte: 100 meter. Minimum hoogte: 2 meter.</p>
  <p>Een dijk met daarop een geluidsscherm wordt aangegeven als dijk/… (zonder de functie “geluid weren”) in combinatie met een inrichtingslijn geluidswering.</p>
  <p>Een wal met daarop een geluidsscherm wordt aangegeven als een inrichtingslijn geluidswering.</p>
</section>"""^^rdf:HTML.


  brt:FunctieWaterdeel
    owl:oneOf
      ( functieWaterdeel:drinkwaterbekken
        functieWaterdeel:haven
        functieWaterdeel:natuurbad
        functieWaterdeel:overig
        functieWaterdeel:viskwekerij
        functieWaterdeel:vistrap
        functieWaterdeel:vloeiveld
        functieWaterdeel:waterval
        functieWaterdeel:waterzuivering
        functieWaterdeel:zwembad );
    rdfs:seeAlso "https://kadaster.github.io/imbrt/#domein-wa_functie"^^xsd:anyURI;
    rdfs:subClassOf skos:Concept;
    skos:definition "De functie van het water."@nl;
    skos:prefLabel "Waterdeel functie"@nl.

  functieWaterdeel:drinkwaterbekken
    skos:definition "Bassin t.b.v. de drinkwatervoorziening."@nl;
    skos:prefLabel "drinkwaterbekken"@nl;
    skos:scopeNote "Ook een spaarbekken (niet noodzakelijk bedoeld voor de drinkwatervoorziening) wordt als drinkwaterbekken ingewonnen."@nl.

  functieWaterdeel:haven
    skos:definition "Lig- en bergplaats voor vaartuigen."@nl;
    skos:prefLabel "haven"@nl;
    skos:scopeNote "Tot welk type water een haven behoort, is afhankelijk van het waterdeel waar de haven aan ligt. Is dat waterdeel een “waterloop” dan is ook de haven een “waterloop”, is dat waterdeel van de categorie “meer, plas, ven, vijver” dan is de haven ook een “meer, plas, ven, vijver”."@nl.

  functieWaterdeel:natuurbad
    skos:definition "Plas in de natuur, ingericht voor zwemmen en zonnebaden."@nl;
    skos:prefLabel "natuurbad"@nl.

  functieWaterdeel:overig
    skos:definition "De waarde van het objectkenmerk is bekend, maar anders dan de genoemde waarden."@nl;
    skos:prefLabel "overig"@nl.

  functieWaterdeel:viskwekerij
    skos:definition "Bassin, aangelegd voor het kweken van vis."@nl;
    skos:prefLabel "viskwekerij"@nl.

  functieWaterdeel:vistrap
    skos:definition "Omleiding bij een stuw of een deel van een waterloop zodanig ingericht dat vissen stroomopwaarts kunnen passeren."@nl;
    skos:prefLabel "viskwekerij"@nl;
    skos:scopeNote "De in de vistrap gelegen stuwtjes worden niet apart aangegeven."@nl.

  functieWaterdeel:vloeiveld
    skos:definition "Omdijkt stuk land, bestemd voor de bezinking van afvalwater."@nl;
    skos:prefLabel "vloeiveld"@nl.

  functieWaterdeel:waterval
    skos:definition "Plaats waar stromend water van een hoogte of helling naar beneden valt."@nl;
    skos:prefLabel "waterval"@nl.

  functieWaterdeel:waterzuivering
    skos:definition "Zuivering van afvalwater."@nl;
    skos:prefLabel "waterzuivering"@nl;
    skos:scopeNote "Een bezinkbak (omdijkt of ommuurd vlak, bestemd voor de bezinking van afvalslib) en beluchtingsbak worden, indien deel uitmakend van een zuiveringsinstallatie, ook opgenomen."@nl.

  functieWaterdeel:zwembad
    skos:definition "De waterbak bij een zwembad."@nl;
    skos:prefLabel "zwembad"@nl;
    skos:scopeNote "Een zwembassin wordt, indien groter dan 50 m² en openbaar toegankelijk, aangegeven als een waterdeel van het type meer/plas/ven/vijver, met de functie zwembad."@nl.



  brt:FysiekVoorkomen
