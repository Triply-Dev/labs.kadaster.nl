    skos:prefLabel "stadion"@nl.

  typeGebouw:stadskantoor_hulpsecretarie
    skos:definition "Bureau waar gemeentelijke instellingen, bijvoorbeeld de burgerlijke stand, gevestigd zijn."@nl;
    skos:prefLabel "stadskantoor, hulpsecretarie"@nl.

  typeGebouw:stationsgebouw
    skos:definition "Een gebouw dat speciaal bedoeld is voor het bedienen van treinreizigers."@nl;
    skos:prefLabel "stationsgebouw"@nl.

  typeGebouw:synagoge
    skos:definition "Bebouwing bedoeld voor joodse geloofsuitoefening."@nl;
