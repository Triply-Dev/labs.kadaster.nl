    skos:prefLabel "veerverbinding"@nl;
    skos:scopeNote """
<section lang="nl">
  <h1>Inwinningscriteria</h1>
  <p>Een veerverbinding komt in verschillende verschijningsvormen voor:</p>
  <ul>
