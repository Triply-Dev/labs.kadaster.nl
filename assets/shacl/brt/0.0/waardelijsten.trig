  brt:Verhardingsbreedteklasse
    owl:oneOf
      ( verhardingsbreedteklasse:2_4meter
        verhardingsbreedteklasse:4_7meter
        verhardingsbreedteklasse:groterdan7meter
        verhardingsbreedteklasse:minderdan2meter );
    rdfs:seeAlso "https://kadaster.github.io/imbrt/#domein-we_verhardingsbreedteklasse"^^xsd:anyURI;
    rdfs:subClassOf skos:Concept;
    skos:definition "De breedte van de verharding, ingedeeld in klassen."@nl;
    skos:prefLabel "Verhardingsbreedteklasse"@nl.

  verhardingsbreedteklasse:2_4meter
    skos:definition "De breedte van de verharding, inclusief beton- en klinkerrand op gelijk niveau als het wegdek, berijdbare goot, geintegreerde rijwielstrook en/of busbaan en parkeerstrook is 2 meter of breder en smaller dan 4 meter."@nl;
    skos:prefLabel "2 - 4 meter"@nl.

  verhardingsbreedteklasse:4_7meter
    skos:definition "De breedte van de verharding, inclusief beton- en klinkerrand op gelijk niveau als het wegdek, berijdbare goot, geintegreerde rijwielstrook en/of busbaan en parkeerstrook is 4 meter of breder en smaller dan 7 meter."@nl;
    skos:prefLabel "4 - 7 meter"@nl.

  verhardingsbreedteklasse:groterdan7meter
    skos:definition "De breedte van de verharding, inclusief beton- en klinkerrand op gelijk niveau als het wegdek, berijdbare goot, geintegreerde rijwielstrook en/of busbaan en parkeerstrook is 7 meter of meer."@nl;
    skos:prefLabel "> 7 meter"@nl.

  verhardingsbreedteklasse:minderdan2meter
    skos:definition "De breedte van de verharding, inclusief beton- en klinkerrand op gelijk niveau als het wegdek, berijdbare goot is smaller dan 2 meter. Een verhardingsbreedteklasse “< 2 meter” geldt alleen voor weglijnen."@nl;
    skos:prefLabel "< 2 meter"@nl.



  brt:Verhardingstype
    owl:oneOf
      ( verhardingstype:halfVerhard
        verhardingstype:onbekend
        verhardingstype:onverhard
        verhardingstype:verhard );
    rdfs:seeAlso "https://kadaster.github.io/imbrt/#domein-we_verhardingstype"^^xsd:anyURI;
    rdfs:subClassOf skos:Concept;
    skos:definition "Het type verharding van een wegdeel."@nl;
    skos:prefLabel "verhardingstype"@nl.

  verhardingstype:halfVerhard
    skos:definition "Een weg waarop de verharding slechts gedeeltelijk is aangebracht (als twee banen met klinkers, beton, tegels, of uitsluitend groenstenen, etc.) of het rijvlak is verbeterd met grind of sintels."@nl;
    skos:prefLabel "half verhard"@nl;
    skos:scopeNote "Wegen met een verharding in slechte staat worden als verhard ingewonnen. Wegen die van nature min of meer steenachtig zijn, zoals bijvoorbeeld in Limburg, worden als onverhard aangegeven."@nl.

  verhardingstype:onbekend
    skos:definition "De waarde van het objectkenmerk is niet bekend."@nl;
    skos:prefLabel "onbekend"@nl.

  verhardingstype:onverhard
    skos:definition "Een weg zonder enige vorm van kunstmatige verharding, dan wel een weg hier en daar opgevuld met puin."@nl;
    skos:prefLabel "onverhard"@nl;
    skos:scopeNote "Ook wegen die van nature min of meer steenachtig zijn, zoals bijvoorbeeld in Limburg, worden als onverhard aangegeven."@nl.

  verhardingstype:verhard
    skos:definition "Een weg met een egale verharding (asfalt, beton, klinkers, tegels, keien, etc.)."@nl;
    skos:prefLabel "verhard"@nl;
    skos:scopeNote "Ook wegen met een verharding in slechte staat worden als verhard ingewonnen. Wegen die van nature min of meer steenachtig zijn, zoals bijvoorbeeld in Limburg, worden als onverhard aangegeven."@nl.



