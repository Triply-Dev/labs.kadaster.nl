    skos:prefLabel "overig"@nl.

  hoofdverkeersgebruik:ruiters
    skos:definition "Weg bestemd voor paardrijders"@nl;
    skos:prefLabel "ruiters"@nl.

  hoofdverkeersgebruik:snelverkeer
